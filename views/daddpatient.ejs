



<!DOCTYPE html>
<html>

<head>
    <title>Doctor Dashboard</title>
    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" />

    <!-- jQuery library -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>

    <!-- Popper JS -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js"></script>
    <link rel="stylesheet" href="/stylesheets/dashboards.css" />

    <!-- Latest compiled JavaScript -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"></script>
</head>

<body class="jumbotron text-center bg-dark body">
    <div class="jumbotron bg-dark">
        <h1 class="text-info display-3">
            Medi<span class="text-secondary">Chain</span>
        </h1>
        <nav class="navbar navbar-expand-sm bg-dark navbar-light btn btn-outline-info">
            <ul class="navbar-nav">
                <li class="nav-item ">
                    <a class="nav-link text-info" href="/">Medichain | Home</a>
                </li>
                <li class="nav-item ">
                    <a class="nav-link text-info" href="/doctor/doctordashboard">Home</a>
                </li>
                <li class="nav-item active">
                  <a class="nav-link text-white" href="/doctor/addpatient"> Add Patient</a>
                </li>
                <li class="nav-item ">
                    <a class="nav-link text-info" href="/doctor/addtreatment"> Add Patient Treatment</a>
                </li>
                <li class="nav-item">
                  <a class="nav-link text-info" href="/doctor/searchtreatment" >Search Treatment </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link text-info" href="/doctor/searchpatient"> Search Patient </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link text-info" href="/doctor/searchdoctor">Search Doctor </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link text-info" href="/doctor/searchhospital">Search Hospitals </a>
                </li>
                
            </ul>
        </nav>
        <br />
        <div class="jumbotron border border-info bg-dark">
            <div class="form-modal">
                <div class="form-toggle">
                  <button id="man-toggle">Add Patient </button>
                </div>
                <div id="man-form">
                  <form id="manform" action="/doctor/register" method="POST">
                    <table>
                      <input type="text" name="DocKey" placeholder="Doctors Key" required>
                      <tr>
                        <td>
                          <input name="Id" type="text" placeholder=" Id" required />
                        </td>
                        <td>
                          <input name="Name" type="text" placeholder=" Name" required />
                        </td>
                      </tr>
                      <tr>
                        <td>
                          <input name="Age" type="text" placeholder="Age" required/>
                        </td>
                        <td>
                          <input name="MyGender" type="text" placeholder="Gender" required />
                        </td>
                      </tr>
                      <tr>
                        <td>
                            <input name="Email" type="text" placeholder="Email" required/>
                          </td>
                          <td>
                            <input name="Phone" type="text" placeholder="Phone" required/>
                          </td>
                      </tr>
                      <tr>
                        <td>
                          <input name="Address" type="text" placeholder="Address" required/>
                        </td>
                        <td>
                          <input type="text" name="Key" placeholder="Address Key" required/>
                        </td>
                      </tr>
                    </table>
                    <button type="submit"  class="btn login"> Add Patient </button>
                  </form>
                </div>
              </div>
        </div>
       
    </div>
</body>

</html>
