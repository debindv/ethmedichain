

  <!DOCTYPE html>
<html>
  <head>
    <title>Hospital| Dashboard</title>
    <!-- Latest compiled and minified CSS -->
    <link
      rel="stylesheet"
      href="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css"
    />

    <!-- jQuery library -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>

    <!-- Popper JS -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js"></script>

    <!-- Latest compiled JavaScript -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"></script>
  </head>

  <body class="jumbotron text-center bg-dark">
    <div class="jumbotron bg-dark">
      <h1 class="text-info display-3">
        Medi<span class="text-secondary ">Chain</span>
      </h1>
      <nav class="navbar navbar-expand-sm bg-dark navbar-light btn btn-outline-info">
          
        <ul class="navbar-nav">
            <li class="nav-item ">
                <a class="nav-link text-info" href="/" >Medichain | Home</a>
              </li>
          <li class="nav-item  ">
            <a class="nav-link text-info" href="/hospital/hospitaldashboard" >Home</a>
          </li>
          <li class="nav-item">
            <a class="nav-link text-info" href="/hospital/add_doctors" > Add Doctors</a>
          </li>
          <li class="nav-item">
            <a class="nav-link text-info" href="/hospital/searchdoctor" > Search Doctor </a>
          </li>
          <li class="nav-item ">
            <a class="nav-link text-info" href="/hospital/searchpatient" > Search Patient </a>
          </li>
          <li class="nav-item active">
            <a class="nav-link text-white" href="/hospital/bill" > Generate Bill</a>
          </li>
          
         
        </ul>
      </nav>
      <br />
    <div class="jumbotron border border-info bg-dark">
        <h2 class="text-info">Treatment Details</h2>

        <table class="table table-sm ">
            <thead class="bg-light text-info">
                <tr>
                    <th>Id</th>
                    <th>Doctor Id</th>
                    <th>Hospital Id</th>
                    <th>Observation</th>
                    <th>Disease</th>
                    <th>Medicine</th>
                </tr>
            </thead>
            <tbody class="text-white">
                <tr>
                    <td>
                        <%= myData._Id1 %>
                    </td>
                    <td>
                        <%= myData._DoctorId %>
                    </td>
                    <td>
                        <%= myData._Hospital %>
                    </td>
                    <td> 
                      <%= myData._Observation%>
                    </td>
                    <td>
                        <%= myData._Disease %>
                    </td>
                    <td>
                        <%= myData._Medicine %>
                     </td>
                     
                </tr> 
            </tbody>
        </table>
    </div>
    <div class="jumbotron border border-info bg-dark">

        <div class="form-modal">
            <div class="form-toggle">
                <button id="man-toggle">Add Doctor</button>
            </div>
            <div id="man-form">
                <form id="manform" action="/hospital/generatebill" method="POST">
                    <table>
                        <input name="HospitalKey" type="text" placeholder="Hospital Address" />

                        <tr>
                            <td>
                                <input name="Id" type="text" placeholder="Patient Id" required />
                            </td>
                            <td><input name="HospitalId" type="text" placeholder="Hospital Id" required/></td>
                        </tr>
                        <tr>
                            <td>
                                <input name="BillId" type="text" placeholder="Bill Id" required />
                            </td>
                            <td>
                                <input name="Medicine" type="text" placeholder="Medicine" required />
                            </td>
                        </tr>
                        
                        <input name="BillAmount" type="text" placeholder="Enter Bill Amount" required/>
                           
                    </table>
                    <button type="submit"  class="btn login">
                        Add Doctor
                    </button>
    
    </div>
 
  </body>
</html>